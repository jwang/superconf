<%= semantic_form_for [:admin, @event, @speaker ], :html => { :multipart => true } do |f| %>
  <%= f.inputs do %>
    <%= f.input :name %>
    <%= f.input :event %>
    <%= f.input :company %>
    <%= f.input :session %>
    <%= f.input :bio %>
    <%= f.input :image, :as => :file %>
    <%= f.input :twitter %>
    <%= f.input :github %>
    <%= f.input :website %>
    <%= f.input :order %>
  <% end %>

  <%= f.action(:submit) %>
<% end %>
